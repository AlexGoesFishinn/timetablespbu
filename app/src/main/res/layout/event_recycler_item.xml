<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    android:layout_marginBottom="16dp">

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:cardCornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="@color/spbu_primary_terracot_color"
        app:strokeWidth="1dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp">

            <RelativeLayout
                android:id="@+id/eventTimeLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp">

                <ImageView
                    android:id="@+id/eventTimeIcon"
                    android:layout_width="16sp"
                    android:layout_height="16sp"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="16dp"
                    android:src="@drawable/ic_time" />

                <TextView
                    android:id="@+id/eventTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_toEndOf="@+id/eventTimeIcon"
                    android:foregroundGravity="center_horizontal"
                    android:textColor="@color/spbu_primary_terracot_color"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    tools:text="12:00" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/eventNameLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/eventTimeLine"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp">

                <ImageView
                    android:id="@+id/eventNameIcon"
                    android:layout_width="16sp"
                    android:layout_height="16sp"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="16dp"
                    android:src="@drawable/ic_event" />

                <TextView
                    android:id="@+id/eventName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_toEndOf="@+id/eventNameIcon"
                    android:foregroundGravity="center_horizontal"
                    android:textSize="16sp"
                    tools:text="Лекция по зельеварению" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/eventSubgroupLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/eventNameLine"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp">

                <ImageView
                    android:id="@+id/eventSubgroupIcon"
                    android:layout_width="16sp"
                    android:layout_height="16sp"
                    android:layout_marginStart="16dp"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_subgroup"
                    />

                <TextView
                    android:id="@+id/eventSubgroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:textSize="16sp"
                    android:layout_toEndOf="@+id/eventSubgroupIcon"
                    android:foregroundGravity="center_horizontal"
                    tools:text="Подгруппа" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/eventSubgroupLine"
                android:id="@+id/eventPlaceLine"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp">

                <ImageView
                    android:id="@+id/eventPlaceIcon"
                    android:layout_width="16sp"
                    android:layout_height="16sp"
                    android:layout_marginStart="16dp"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_location" />

                <TextView
                    android:id="@+id/eventPlace"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:textSize="16sp"
                    android:layout_toEndOf="@+id/eventPlaceIcon"
                    android:foregroundGravity="center_horizontal"
                    tools:text="Хогвартс" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/eventPlaceLine"
                android:id="@+id/eventEducatorLine"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp">

                <ImageView
                    android:id="@+id/eventEducatorIcon"
                    android:layout_width="16sp"
                    android:layout_height="16sp"
                    android:layout_marginStart="16dp"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_educator" />

                <TextView
                    android:id="@+id/eventLecturer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:textSize="16sp"
                    android:layout_toEndOf="@+id/eventEducatorIcon"
                    android:foregroundGravity="center_horizontal"
                    tools:text="Профессор Снейп" />


            </RelativeLayout>


        </RelativeLayout>


